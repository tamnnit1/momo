{
    "name": "tamnnit/onepay",
    "type": "library",
    "description": "payment client MoMo.",
    "keywords": [
        "momo",
        "payment"
    ],
    "homepage": "https://github.com/tamnnit1/momo",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Nhat Tam",
            "email": "ntam444@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "omnipay/common": "^3.0",
        "php-http/guzzle6-adapter": "^1.1"    
    },
    "require-dev": {
        "omnipay/tests": "^3.0",
        "scrutinizer/ocular": "^1.5"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\MoMo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\MoMo\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "\"vendor/bin/phpunit\""
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
	}
}
